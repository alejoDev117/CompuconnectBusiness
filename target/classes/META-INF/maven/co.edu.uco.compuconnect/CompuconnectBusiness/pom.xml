<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>co.edu.uco.compuconnect</groupId>
  <artifactId>CompuconnectBusiness</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <name>CompuconnectBusiness</name>
  <description>Componente encargado de gestionar las clases que contienen la logica del negocio.</description>
  <build>
    <sourceDirectory>src</sourceDirectory>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.8.1</version>
        <configuration>
          <release>19</release>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <dependencies> 
	  <dependency>
		    <groupId>co.edu.uco.compuconnect</groupId>
  			<artifactId>CompuconnectEntities</artifactId>
  			<version>0.0.1-SNAPSHOT</version>
	  </dependency>
	  <dependency>
  		<groupId>co.edu.uco.compuconnect</groupId>
  		<artifactId>CompuconnectDTO</artifactId>
  		<version>0.0.1-SNAPSHOT</version>
	  </dependency>
	  <dependency>
		  <groupId>co.edu.uco.compuconnect</groupId>
  		  <artifactId>CompuconnectCrossCutting</artifactId>
  		  <version>0.0.1-SNAPSHOT</version>
	  </dependency>
	  <dependency>
		    <groupId>co.edu.uco.compuconnect</groupId>
  			<artifactId>CompuconnectData</artifactId>
  			<version>0.0.1-SNAPSHOT</version>
	  </dependency>
  </dependencies>
</project>